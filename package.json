{
  "name": "keyv-hana",
  "version": "1.0.0",
  "description": "SAP HANA storage adapter for Keyv",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "rimraf ./dist && tsup src/index.ts --format cjs,esm --dts --clean",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rimraf ./node_modules ./coverage ./dist"
  },
  "keywords": [
    "keyv",
    "hana",
    "sap",
    "sap-hana",
    "storage",
    "adapter",
    "cache",
    "key-value"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikezaschka/keyv-hana"
  },
  "peerDependencies": {
    "@sap/hana-client": ">=2.0.0"
  },
  "devDependencies": {
    "@keyv/test-suite": "^2.0.0",
    "@sap/hana-client": "^2.21.31",
    "@types/node": "^25.2.3",
    "@vitest/coverage-v8": "^3.0.0",
    "dotenv": "^17.3.1",
    "keyv": "^5.0.0",
    "rimraf": "^6.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.0.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "files": [
    "dist",
    "LICENSE"
  ]
}
